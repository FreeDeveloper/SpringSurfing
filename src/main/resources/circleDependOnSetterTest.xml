<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!--Setter方法依赖-->
    <bean id="testAcos" class="cn.smile.spring.container.bean.circleDepend.ClassA" scope="singleton">
        <property name="testb" ref="testBcos"></property>
    </bean>

    <bean id="testBcos" class="cn.smile.spring.container.bean.circleDepend.ClassB" scope="singleton">
        <property name="testc" ref="testCcos"></property>
    </bean>

    <bean id="testCcos" class="cn.smile.spring.container.bean.circleDepend.ClassC" scope="singleton">
        <property name="testa" ref="testAcos"></property>
    </bean>

</beans>